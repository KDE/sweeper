Licensed under the terms of the LGPL.
Derived from AT&amp;T's Privacy Bird settings panel screenshot originally.
</comment>
  <exportmacro></exportmacro>
  <class>KPrivacySettings</class>
  <widget class="QWidget" name="KPrivacySettings" >
    <property name="geometry" >
      <rect>
        <x>0</x>
        <y>0</y>
        <width>905</width>
        <height>724</height>
      </rect>
    </property>
    <property name="windowTitle" >
      <string>Privacy Settings</string>
    </property>
    <layout class="QGridLayout" >
      <item rowspan="1" row="1" column="0" colspan="2" >
        <widget class="Line" name="line1" >
          <property name="frameShape" >
            <enum>QFrame::HLine</enum>
          </property>
          <property name="frameShadow" >
            <enum>QFrame::Sunken</enum>
          </property>
        </widget>
      </item>
      <item row="0" column="0" >
        <widget class="QLabel" name="textLabel1" >
          <property name="text" >
            <string>Network privacy level:</string>
          </property>
          <property name="buddy" stdset="0" >
            <cstring>_privacyLevel</cstring>
          </property>
          <property name="wordWrap" >
            <bool>false</bool>
          </property>
        </widget>
      </item>
      <item row="0" column="1" >
        <widget class="QComboBox" name="_privacyLevel" >
          <property name="sizePolicy" >
            <sizepolicy>
              <hsizetype>7</hsizetype>
              <vsizetype>0</vsizetype>
              <horstretch>0</horstretch>
              <verstretch>0</verstretch>
            </sizepolicy>
          </property>
          <item>
            <property name="text" >
              <string>Low</string>
            </property>
          </item>
          <item>
            <property name="text" >
              <string>Medium</string>
            </property>
          </item>
          <item>
            <property name="text" >
              <string>High</string>
            </property>
          </item>
          <item>
            <property name="text" >
              <string>Custom</string>
            </property>
          </item>
        </widget>
      </item>
      <item rowspan="1" row="6" column="0" colspan="2" >
        <widget class="Q3ButtonGroup" name="buttonGroup1_2" >
          <property name="title" >
            <string>Financial Information</string>
          </property>
          <layout class="QVBoxLayout" >
            <property name="margin" >
              <number>0</number>
            </property>
            <item>
              <widget class="QLabel" name="textLabel2" >
                <property name="text" >
                  <string>Warn me when I visit a site that uses my financial or purchase information:</string>
                </property>
                <property name="wordWrap" >
                  <bool>false</bool>
                </property>
              </widget>
            </item>
            <item>
              <widget class="QCheckBox" name="_warnFinancialMarketeer" >
                <property name="text" >
                  <string>For marketing or advertising purposes</string>
                </property>
              </widget>
            </item>
            <item>
              <widget class="QCheckBox" name="_warnFinancialSpammer" >
                <property name="text" >
                  <string>To share with other companies</string>
                </property>
              </widget>
            </item>
          </layout>
        </widget>
      </item>
      <item rowspan="1" row="5" column="0" colspan="2" >
        <widget class="Q3ButtonGroup" name="buttonGroup1" >
          <property name="title" >
            <string>Health Information</string>
          </property>
          <layout class="QVBoxLayout" >
            <property name="margin" >
              <number>0</number>
            </property>
            <item>
              <widget class="QLabel" name="textLabel2_2" >
                <property name="text" >
                  <string>Warn me when I visit a site that uses my health or medical information: </string>
                </property>
                <property name="wordWrap" >
                  <bool>false</bool>
                </property>
              </widget>
            </item>
            <item>
              <widget class="QCheckBox" name="_warnHealthMarketeer" >
                <property name="text" >
                  <string>For marketing or advertising purposes</string>
                </property>
              </widget>
            </item>
            <item>
              <widget class="QCheckBox" name="_warnHealthSpammer" >
                <property name="text" >
                  <string>To share with other companies</string>
                </property>
              </widget>
            </item>
          </layout>
        </widget>
      </item>
      <item rowspan="1" row="7" column="0" colspan="2" >
        <widget class="Q3ButtonGroup" name="buttonGroup1_2_2" >
          <property name="title" >
            <string>Demographics</string>
          </property>
          <layout class="QVBoxLayout" >
            <property name="margin" >
              <number>0</number>
            </property>
            <item>
              <widget class="QLabel" name="textLabel2_3" >
                <property name="text" >
                  <string>Warn me when I visit a site that uses my non-personally identifiable information:</string>
                </property>
                <property name="wordWrap" >
                  <bool>false</bool>
                </property>
              </widget>
            </item>
            <item>
              <widget class="QCheckBox" name="_warnDemographicsBigBrother" >
                <property name="text" >
                  <string>To determine my interests, habits or general behavior</string>
                </property>
              </widget>
            </item>
            <item>
              <widget class="QCheckBox" name="_warnDemographicsSpammer" >
                <property name="text" >
                  <string>To share with other companies</string>
                </property>
              </widget>
            </item>
          </layout>
        </widget>
      </item>
      <item rowspan="1" row="2" column="0" colspan="2" >
        <widget class="QCheckBox" name="_warnSpammers" >
          <property name="text" >
            <string>Warn me when I visit a site that shares my personal information with other companies</string>
          </property>
        </widget>
      </item>
      <item rowspan="1" row="3" column="0" colspan="2" >
        <widget class="QCheckBox" name="_warnShady" >
          <property name="text" >
            <string>Warn me when I visit a site that does not let me know what information they have about me</string>
          </property>
        </widget>
      </item>
      <item rowspan="1" row="4" column="0" colspan="2" >
        <widget class="Q3ButtonGroup" name="buttonGroup4" >
          <property name="title" >
            <string>Personal Information</string>
          </property>
          <layout class="QGridLayout" >
            <property name="margin" >
              <number>0</number>
            </property>
            <item rowspan="1" row="0" column="0" colspan="2" >
              <widget class="QLabel" name="textLabel2_2_2" >
                <property name="text" >
                  <string>Warn me when I visit a site that may contact me about other products or services:</string>
                </property>
                <property name="wordWrap" >
                  <bool>false</bool>
                </property>
              </widget>
            </item>
            <item rowspan="1" row="2" column="0" colspan="2" >
              <widget class="QLabel" name="textLabel2_2_2_2" >
                <property name="text" >
                  <string>Warn me when I visit a site that may use my personal information to:</string>
                </property>
                <property name="wordWrap" >
                  <bool>false</bool>
                </property>
              </widget>
            </item>
            <item rowspan="1" row="3" column="0" colspan="2" >
              <widget class="QCheckBox" name="_warnPersonalHabits" >
                <property name="text" >
                  <string>Determine my habits, interests or general behavior</string>
                </property>
              </widget>
            </item>
            <item row="1" column="0" >
              <layout class="QVBoxLayout" >
                <property name="margin" >
                  <number>0</number>
                </property>
                <item>
                  <widget class="QCheckBox" name="_warnPersonalTelephone" >
                    <property name="text" >
                      <string>Via telephone</string>
                    </property>
                  </widget>
                </item>
                <item>
                  <widget class="QCheckBox" name="_warnPersonalMail" >
                    <property name="text" >
                      <string>Via mail</string>
                    </property>
                  </widget>
                </item>
              </layout>
            </item>
            <item row="1" column="1" >
              <layout class="QVBoxLayout" >
                <property name="margin" >
                  <number>0</number>
                </property>
                <item>
                  <widget class="QCheckBox" name="_warnPersonalEmail" >
                    <property name="text" >
                      <string>Via email</string>
                    </property>
                  </widget>
                </item>
                <item>
                  <widget class="QCheckBox" name="_warnPersonalBigBrother" >
                    <property name="text" >
                      <string>And do not allow me to remove my contact information</string>
                    </property>
                  </widget>
                </item>
              </layout>
            </item>
          </layout>
        </widget>
      </item>
    </layout>
  </widget>
  <layoutdefault spacing="3" margin="6" />
  <layoutfunction spacing="KDialog::spacingHint" margin="KDialog::marginHint" />
  <pixmapfunction>qPixmapFromMimeSource</pixmapfunction>
</ui>
