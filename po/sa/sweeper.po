#: main.cpp:35 privacyfunctions.h:65
#, kde-format
msgid "Thumbnail Cache"
msgstr "लघुचित्र संग्रह"

#: main.cpp:42
#, kde-format
msgid "Sweeps without user interaction"
msgstr "उपयोक्तृपरस्परक्रियां विना स्वीपं करोति"

#: privacyfunctions.cpp:44 privacyfunctions.cpp:53 privacyfunctions.cpp:62
#, kde-format
msgid "A thumbnail could not be removed."
msgstr "लघुचित्रं निष्कासयितुं न शक्यते स्म ।"

#: privacyfunctions.cpp:131
#, kde-format
msgid "The file exists but could not be removed."
msgstr "सञ्चिका अस्ति किन्तु निष्कासयितुं न शक्यते स्म ।"

#: privacyfunctions.cpp:210
#, kde-format
msgid "A favicon could not be removed."
msgstr "एकः फेविकोन् अपसारयितुं न शक्तवान् ।"

#: privacyfunctions.h:22
#, kde-format
msgid "Cookies"
msgstr "कुकीज"

#: privacyfunctions.h:22
#, kde-format
msgid "Clears all stored cookies set by websites"
msgstr "वेबसाइट् द्वारा सेट् कृतानि सर्वाणि संगृहीतकुकीजं स्वच्छं करोति"

#: privacyfunctions.h:37
#, kde-format
msgid "Cookie Policies"
msgstr "कुकी नीतयः"

#: privacyfunctions.h:37
#, kde-format
msgid "Clears the cookie policies for all visited websites"
msgstr "सर्वेषां भ्रमणीयजालस्थलानां कुकीनीतयः स्वच्छं करोति"

#: privacyfunctions.h:51
#, kde-format
msgid "Saved Clipboard Contents"
msgstr "रक्षित क्लिपबोर्ड सामग्री"

#: privacyfunctions.h:51
#, kde-format
msgid "Clears the clipboard contents stored by Klipper"
msgstr "Klipper द्वारा संगृहीतं क्लिप्बोर्ड् सामग्रीं स्वच्छं करोति"

#: privacyfunctions.h:65
#, kde-format
msgid "Clears all cached thumbnails"
msgstr "सर्वाणि संग्रहीतानि लघुचित्रं स्वच्छं करोति"

#: privacyfunctions.h:79
#, kde-format
msgid "Run Command History"
msgstr "आदेश इतिहासं चालयन्तु"

#: privacyfunctions.h:79
#, kde-format
msgid ""
"Clears the history of commands run through the Run Command tool on the "
"desktop"
msgstr ""
"डेस्कटॉप् मध्ये Run Command tool इत्यस्य माध्यमेन चालितानां आदेशानां इतिहासं स्वच्छं करोति"

#: privacyfunctions.h:93
#, kde-format
msgid "Form Completion Entries"
msgstr "प्रपत्र समाप्ति प्रविष्टियाँ"

#: privacyfunctions.h:93
#, kde-format
msgid "Clears values which were entered into forms on websites"
msgstr "मूल्यानि स्वच्छं करोति ये जालपुटेषु प्रपत्रेषु प्रविष्टाः आसन्"

#: privacyfunctions.h:107
#, kde-format
msgid "Web History"
msgstr "जाल इतिहास"

#: privacyfunctions.h:107
#, kde-format
msgid "Clears the history of visited websites"
msgstr "आगतानां जालपुटानां इतिहासं स्वच्छं करोति"

#: privacyfunctions.h:121
#, kde-format
msgid "Web Cache"
msgstr "Web Cache"

#: privacyfunctions.h:121
#, kde-format
msgid "Clears the temporary cache of websites visited"
msgstr "आगतानां वेबसाइट्-स्थानानां अस्थायी-सञ्चयं स्वच्छं करोति"

#: privacyfunctions.h:135
#, kde-format
msgid "Recent Documents"
msgstr "अद्यतन दस्तावेज"

#: privacyfunctions.h:135
#, kde-format
msgid ""
"Clears the list of recently used documents from the KDE applications menu"
msgstr "KDE अनुप्रयोगमेनूतः सद्यः प्रयुक्तानां दस्तावेजानां सूचीं स्वच्छं करोति"

#: privacyfunctions.h:149
#, kde-format
msgid "Favorite Icons"
msgstr "प्रिय चिह्न"

#: privacyfunctions.h:149
#, kde-format
msgid "Clears the FavIcons cached from visited websites"
msgstr "आगतजालस्थलेभ्यः संग्रहीतं FavIcons स्वच्छं करोति"

#: privacyfunctions.h:163
#, kde-format
msgid "Recent Applications"
msgstr "हाल के आवेदन"

#: privacyfunctions.h:163
#, kde-format
msgid "Clears the list of recently used applications from KDE menu"
msgstr "KDE मेन्यूतः सद्यः प्रयुक्तानां अनुप्रयोगानाम् सूचीं स्वच्छं करोति"

